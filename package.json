{
  "name": "twotter",
  "version": "0.0.1",
  "description": "Simple Twitter like app with React/Redux, AirBnB ESLint, Passport Local and Mocha/Chai unit tests. ",
  "main": "index.js",
  "repository": "git@github.com:roelver/twotter.git",
  "scripts": {
    "dev": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "lint": "eslint src",
    "test:server": "mocha --compilers js:babel-core/register,babel-polyfill  --recursive ./test/server",
    "test:ui": "mocha --compilers js:babel-core/register,babel-polyfill --require ./test/test_helper.js --recursive ./test/components",
    "test:uiwatch": "npm run test:ui -- --watch",
    "test:serverwatch": "npm run test:server -- --watch",
    "postinstall": "webpack -p",
    "start": "node server.js",
    "startserver": "node server/index.js",
    "devserver": "nodemon server/index.js"
  },
  "author": "Roel Verbunt",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.2.1",
    "babel-loader": "^7.1.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.0",
    "chai-jquery": "^2.0.0",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "jquery": "^3.2.1",
    "jsdom": "^11.1.0",
    "mocha": "^3.4.2",
    "sinon": "^3.2.1",
    "supertest": "^3.0.0",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-stage-1": "^6.1.18",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.17.2",
    "cors": "^2.8.4",
    "express": "^4.15.4",
    "history": "^4.7.2",
    "jwt-simple": "^0.5.1",
    "lodash": "^4.17.4",
    "mongoose": "^4.11.6",
    "morgan": "^1.8.2",
    "passport": "^0.4.0",
    "passport-jwt": "^2.2.1",
    "passport-local": "^1.0.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-animate-height": "^0.10.2",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-transition-group": "^1.2.0",
    "redux": "^3.0.4",
    "redux-form": "^7.0.3",
    "redux-thunk": "^2.2.0"
  }
}
